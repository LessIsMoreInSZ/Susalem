<UserControl
    x:Class="susalem.wpf.Views.ThreeDView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helix="http://helix-toolkit.org/wpf"
    xmlns:local="clr-namespace:susalem.wpf.Visual3D"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="800"
    d:DesignWidth="1000"
    mc:Ignorable="d">
    <Grid>
        <helix:HelixViewport3D Name="vp" IsHeadLightEnabled="True">
            <local:ChildSelectableVisual3D x:Name="cv"
                MouseLeftButtonDown="efem_MouseLeftButtonDown" 
         />        
        </helix:HelixViewport3D>
    </Grid>
</UserControl>
